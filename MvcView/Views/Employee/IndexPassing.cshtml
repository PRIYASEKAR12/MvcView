@model EmployeeEntity.Employee

@{
    ViewBag.Title = "IndexPassing";
}
<h2>Employee Details</h2>
<h3> Employee Details-ViewData</h3>
<table border="1">
    <tr>
        <td>Employee Name</td>
        <td>Employee PhoneNumber</td>
    </tr>
    @foreach (EmployeeEntity.Employee item in (List<EmployeeEntity.Employee>)ViewData["employees"])
    {
        <tr>
            <td>
                @item.EmployeeName
            </td>
            <td>
                @item.EmployeePhoneNumber
            </td>
        </tr>
    }
</table>
<h3>Employee Details-ViewBag</h3>
<table border="1">
    <tr>
        <td>Employee Name</td>
        <td>Employee PhoneNumber</td>
    </tr>
    @foreach (EmployeeEntity.Employee item in ViewBag.Employee)
    {
        <tr>
            <td>
                @item.EmployeeName
            </td>
            <td>
                @item.EmployeePhoneNumber
            </td>
        </tr>
    }
</table>